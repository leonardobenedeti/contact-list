    "version": "0.2.0",
    "configurations": [
        {
            "name": "App",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define",
                "environment=development",
                "--web-port=4200",
                "--web-renderer web",
                "--web-browser-flag=--disable-web-security",
            ]
        },
        {
            "name": "Storybook",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_storybook.dart",
            "args": [
                "--dart-define",
                "environment=development",
                "--web-port=4200",
                "--web-renderer web",
                "--web-browser-flag=--disable-web-security",
            ]
        },
    ]
}